<?xml version="1.0" encoding="utf-8"?>
<odoo>
     <record id="view_warehouse_inherit_l10n_in_stock" model="ir.ui.view">
        <field name="name">stock.warehouse.inherit.l10n.in.stock</field>
        <field name="model">stock.warehouse</field>
        <field name="inherit_id" ref="stock.view_warehouse"/>
        <field name="arch" type="xml">
            <field name="partner_id" position="after">
                <field name="l10n_in_gstin_partner_id"  groups='l10n_in.group_l10n_in_multi_gstin' options="{'no_create': True, 'always_reload': True}" context="{'show_vat':True}" domain="[('l10n_in_gstin_company_id', '=', company_id)]"/>
            </field>
        </field>
    </record>
</odoo>
